-- Dual-Key Panel (Fixed Center)
-- Place this LocalScript under StarterGui (ScreenGui)

local Players = game:GetService("Players")
local HttpService = game:GetService("HttpService")
local player = Players.LocalPlayer

-- CONFIG: set your keys and script URLs here
local KEY_NORMAL = "normal"
local KEY_PREMIUM = "premkey"
local SCRIPT_URL_NORMAL = "https://raw.githubusercontent.com/roxaspp/JoshHub/refs/heads/main/JoshHubNormalKeyLoadstring"
local SCRIPT_URL_PREMIUM = "https://raw.githubusercontent.com/roxaspp/JoshHub/refs/heads/main/joshhubpremium"
local DISCORD_INVITE = "https://discord.gg/n2P7pxvq"

-- CREATE SCREEN GUI
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "JoshPanelKeyüîê"
screenGui.ResetOnSpawn = false
screenGui.Parent = player:WaitForChild("PlayerGui")

-- FRAME (fixed sa gitna)
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 325, 0, 180)
frame.Position = UDim2.new(0.5, 0, 0.5, 0)
frame.AnchorPoint = Vector2.new(0.5, 0.5) -- fixed center
frame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
frame.BorderSizePixel = 0
frame.Parent = screenGui

local uiCorner = Instance.new("UICorner", frame)
uiCorner.CornerRadius = UDim.new(0, 10)

-- TITLE
local title = Instance.new("TextLabel")
title.Text = "Dual Key Panel"
title.Size = UDim2.new(1, -20, 0, 36)
title.Position = UDim2.new(0, 10, 0, 10)
title.BackgroundTransparency = 1
title.TextColor3 = Color3.fromRGB(255,255,255)
title.Font = Enum.Font.SourceSansBold
title.TextSize = 20
title.Parent = frame

-- KEYBOX
local keyBox = Instance.new("TextBox")
keyBox.PlaceholderText = "Ilagay ang key dito"
keyBox.Size = UDim2.new(1, -20, 0, 36)
keyBox.Position = UDim2.new(0, 10, 0, 56)
keyBox.BackgroundColor3 = Color3.fromRGB(45,45,45)
keyBox.TextColor3 = Color3.fromRGB(230,230,230)
keyBox.Font = Enum.Font.SourceSans
keyBox.TextSize = 18
keyBox.ClearTextOnFocus = false
keyBox.Parent = frame
local keyCorner = Instance.new("UICorner", keyBox)
keyCorner.CornerRadius = UDim.new(0,6)

-- BUTTON CREATION
local function makeButton(text, posY)
    local b = Instance.new("TextButton")
    b.Text = text
    b.Size = UDim2.new(1, -20, 0, 36)
    b.Position = UDim2.new(0, 10, 0, posY)
    b.BackgroundColor3 = Color3.fromRGB(60,60,60)
    b.TextColor3 = Color3.fromRGB(255,255,255)
    b.Font = Enum.Font.SourceSansBold
    b.TextSize = 16
    b.Parent = frame
    local c = Instance.new("UICorner", b)
    c.CornerRadius = UDim.new(0,6)
    return b
end

local normalBtn = makeButton("Use Normal Key", 100)
local premiumBtn = makeButton("Use Premium Key", 140)

-- GET KEY BUTTON
local getKeyBtn = Instance.new("TextButton")
getKeyBtn.Text = "Get Key / Copy Discord"
getKeyBtn.Size = UDim2.new(0, 160, 0, 30)
getKeyBtn.Position = UDim2.new(1, -170, 0, 10)
getKeyBtn.AnchorPoint = Vector2.new(1, 0)
getKeyBtn.BackgroundColor3 = Color3.fromRGB(50,50,50)
getKeyBtn.TextColor3 = Color3.fromRGB(255,255,255)
getKeyBtn.Font = Enum.Font.SourceSansBold
getKeyBtn.TextSize = 14
getKeyBtn.Parent = frame
local gkCorner = Instance.new("UICorner", getKeyBtn)
gkCorner.CornerRadius = UDim.new(0,6)

-- FEEDBACK LABEL
local feedback = Instance.new("TextLabel")
feedback.Text = ""
feedback.Size = UDim2.new(1, -20, 0, 20)
feedback.Position = UDim2.new(0, 10, 1, -30)
feedback.BackgroundTransparency = 1
feedback.TextColor3 = Color3.fromRGB(200,200,200)
feedback.Font = Enum.Font.SourceSans
feedback.TextSize = 14
feedback.TextXAlignment = Enum.TextXAlignment.Left
feedback.Parent = frame

local function setFeedback(txt, timeout)
    feedback.Text = txt
    if timeout then
        delay(timeout, function()
            if feedback then feedback.Text = "" end
        end)
    end
end

-- HELPER: fetch and execute script
local function fetchAndExecute(url)
    if url == "" or not url then
        setFeedback("Script URL hindi naka-set.", 4)
        return
    end
    if not HttpService.HttpEnabled then
        setFeedback("HttpService naka-off. I-enable muna para magamit.", 5)
        return
    end
    spawn(function()
        local ok, result = pcall(function() return game:HttpGet(url) end)
        if not ok then
            setFeedback("Failed to download script: "..tostring(result), 6)
            return
        end
        local ok2, fn = pcall(function() return loadstring(result) end)
        if not ok2 or typeof(fn) ~= "function" then
            setFeedback("Loadstring error o wala function.", 6)
            return
        end
        local ok3, res3 = pcall(fn)
        if not ok3 then
            setFeedback("Error habang pinapatakbo ang script: "..tostring(res3), 6)
            return
        end
        setFeedback("Script na-run nang successful!", 4)
    end)
end

-- BUTTON ACTIONS
normalBtn.MouseButton1Click:Connect(function()
    local input = tostring(keyBox.Text or "")
    if input == KEY_NORMAL then
        setFeedback("Normal key accepted. Downloading...", 3)
        fetchAndExecute(SCRIPT_URL_NORMAL)
    else
        setFeedback("Invalid normal key.", 4)
    end
end)

premiumBtn.MouseButton1Click:Connect(function()
    local input = tostring(keyBox.Text or "")
    if input == KEY_PREMIUM then
        setFeedback("Premium key accepted. Downloading premium script...", 3)
        fetchAndExecute(SCRIPT_URL_PREMIUM)
    else
        setFeedback("Invalid premium key.", 4)
    end
end)

getKeyBtn.MouseButton1Click:Connect(function()
    local ok, err = pcall(function()
        if setclipboard then
            setclipboard(DISCORD_INVITE)
            return true
        end
        return false
    end)
    if ok and err == true then
        setFeedback("Discord invite copied to clipboard!", 4)
    else
        keyBox.Text = DISCORD_INVITE
        setFeedback("Discord invite inilagay sa box ‚Äî i-Ctrl+C para kopyahin.", 6)
    end
end)

-- INITIAL FEEDBACK
setFeedback("Ready. Ilagay ang key at piliin Normal o Premium.", 6)
