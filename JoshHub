--// Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer
local PlayerGui = LocalPlayer:WaitForChild("PlayerGui")

--// Keys and their corresponding scripts
local KeyToScript = {
    ["NORMAL123"] = function()
        print("Normal Script Running!")
        -- Dito ilagay ang buong normal script mo
    end,
    ["PREMIUM456"] = function()
        print("Premium Script Running!")
        -- Dito ilagay ang buong premium script mo
    end
}

--// Create ScreenGui
local screenGui = Instance.new("ScreenGui")
screenGui.Name = "KeyPanel"
screenGui.ResetOnSpawn = false
screenGui.Parent = PlayerGui

--// Main Frame (Fit sa CP)
local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 300, 0, 150)
frame.Position = UDim2.new(0.5, -150, 0.5, -75)
frame.BackgroundColor3 = Color3.fromRGB(30,30,30)
frame.BorderSizePixel = 0
frame.Parent = screenGui

--// Logo (Placeholder)
local logo = Instance.new("ImageLabel")
logo.Size = UDim2.new(0, 40, 0, 40)
logo.Position = UDim2.new(0, 10, 0, 10)
logo.BackgroundTransparency = 1
logo.Image = "" -- ilagay ang logo link dito
logo.Parent = frame

--// Title
local title = Instance.new("TextLabel")
title.Size = UDim2.new(1, -60, 0, 30)
title.Position = UDim2.new(0, 60, 0, 10)
title.BackgroundTransparency = 1
title.Text = "Enter Your Key"
title.TextColor3 = Color3.fromRGB(255,255,255)
title.TextScaled = true
title.TextXAlignment = Enum.TextXAlignment.Left
title.Parent = frame

--// Input Box
local inputBox = Instance.new("TextBox")
inputBox.Size = UDim2.new(0.8,0,0,40)
inputBox.Position = UDim2.new(0.1,0,0,50)
inputBox.PlaceholderText = "Type your key here"
inputBox.Text = ""
inputBox.ClearTextOnFocus = true
inputBox.TextScaled = true
inputBox.Parent = frame

--// Submit Button
local submitButton = Instance.new("TextButton")
submitButton.Size = UDim2.new(0.8,0,0,40)
submitButton.Position = UDim2.new(0.1,0,0,100)
submitButton.Text = "Submit Key"
submitButton.TextScaled = true
submitButton.BackgroundColor3 = Color3.fromRGB(50,50,50)
submitButton.TextColor3 = Color3.fromRGB(255,255,255)
submitButton.Parent = frame

--// Button Function
submitButton.MouseButton1Click:Connect(function()
    local userKey = inputBox.Text
    if KeyToScript[userKey] then
        KeyToScript[userKey]()  -- Run corresponding script
        frame:Destroy()          -- Destroy panel after success
    else
        print("Invalid Key!")
    end
end)
