--// JoshHub Dual Key Loader (Simplified + Working)
--// Owner: JoshroxasScript

--// Services
local Players = game:GetService("Players")
local LocalPlayer = Players.LocalPlayer

--// CONFIG (palitan mo)
local NormalKey = "Normal123"         -- üîë Normal Key
local PremiumKey = "Premium123@GFall" -- üîë Premium Key
local DiscordInvite = "https://discord.gg/YourServer" -- Link ng server mo

-- Mga link ng scripts (palitan mo ng Pastebin/Gist)
local NormalScriptURL = "https://raw.githubusercontent.com/roxaspp/JoshHub/refs/heads/main/JoshHubNormalKeyLoadstring"
local PremiumScriptURL = "https://raw.githubusercontent.com/roxaspp/JoshHub/refs/heads/main/joshhubpremium"

--// UI Setup
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "JoshHubKeyUI"

-- parent fix para sa lahat ng executor
if gethui then
    ScreenGui.Parent = gethui()
else
    ScreenGui.Parent = game.CoreGui
end

local KeyFrame = Instance.new("Frame", ScreenGui)
KeyFrame.Size = UDim2.new(0, 320, 0, 180)
KeyFrame.Position = UDim2.new(0.5, -160, 0.5, -90)
KeyFrame.BackgroundColor3 = Color3.fromRGB(30, 30, 30)

local UICorner = Instance.new("UICorner", KeyFrame)
UICorner.CornerRadius = UDim.new(0, 10)

local Title = Instance.new("TextLabel", KeyFrame)
Title.Size = UDim2.new(1, 0, 0, 30)
Title.BackgroundTransparency = 1
Title.Text = "üîë JoshHub Key System"
Title.Font = Enum.Font.SourceSansBold
Title.TextSize = 18
Title.TextColor3 = Color3.new(1, 1, 0.6)

local KeyBox = Instance.new("TextBox", KeyFrame)
KeyBox.Size = UDim2.new(0.9, 0, 0, 40)
KeyBox.Position = UDim2.new(0.05, 0, 0.25, 0)
KeyBox.PlaceholderText = "Enter Key..."
KeyBox.Text = ""
KeyBox.BackgroundColor3 = Color3.fromRGB(50, 50, 50)
KeyBox.TextColor3 = Color3.new(1, 1, 1)

local SubmitBtn = Instance.new("TextButton", KeyFrame)
SubmitBtn.Size = UDim2.new(0.4, 0, 0, 35)
SubmitBtn.Position = UDim2.new(0.05, 0, 0.65, 0)
SubmitBtn.Text = "‚úÖ Submit"
SubmitBtn.BackgroundColor3 = Color3.fromRGB(0, 170, 0)
SubmitBtn.TextColor3 = Color3.new(1, 1, 1)

local GetKeyBtn = Instance.new("TextButton", KeyFrame)
GetKeyBtn.Size = UDim2.new(0.4, 0, 0, 35)
GetKeyBtn.Position = UDim2.new(0.55, 0, 0.65, 0)
GetKeyBtn.Text = "üìã Get Key"
GetKeyBtn.BackgroundColor3 = Color3.fromRGB(0, 85, 170)
GetKeyBtn.TextColor3 = Color3.new(1, 1, 1)

local StatusLabel = Instance.new("TextLabel", KeyFrame)
StatusLabel.Size = UDim2.new(1, 0, 0, 25)
StatusLabel.Position = UDim2.new(0, 0, 0.85, 0)
StatusLabel.BackgroundTransparency = 1
StatusLabel.TextColor3 = Color3.new(1, 0, 0)
StatusLabel.Text = ""

-- Copy Discord link
GetKeyBtn.MouseButton1Click:Connect(function()
    if setclipboard then
        setclipboard(DiscordInvite)
    end
    StatusLabel.TextColor3 = Color3.new(0.5, 0.8, 1)
    StatusLabel.Text = "üìã Discord invite copied!"
end)

-- Key Check
SubmitBtn.MouseButton1Click:Connect(function()
    local inputKey = KeyBox.Text

    if inputKey == NormalKey then
        StatusLabel.TextColor3 = Color3.new(0, 1, 0)
        StatusLabel.Text = "‚úÖ Normal Key Accepted!"
        task.wait(1)
        ScreenGui:Destroy()
        loadstring(game:HttpGet(NormalScriptURL))()

    elseif inputKey == PremiumKey then
        StatusLabel.TextColor3 = Color3.new(1, 0.8, 0)
        StatusLabel.Text = "‚≠ê Premium Key Accepted!"
        task.wait(1)
        ScreenGui:Destroy()
        loadstring(game:HttpGet(PremiumScriptURL))()

    else
        StatusLabel.TextColor3 = Color3.new(1, 0, 0)
        StatusLabel.Text = "‚ùå Invalid Key!"
    end
end)
