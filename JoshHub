-- Services
local Players = game:GetService("Players")
local TweenService = game:GetService("TweenService")
local LocalPlayer = Players.LocalPlayer

-- CONFIG
local NormalKey = "Normal123"         -- üîë Normal Key
local PremiumKey = "Premium123@GFall" -- üîë Premium Key
local DiscordInvite = "https://discord.gg/YourServer" -- Discord link

-- Script URLs (palitan mo ng actual links mo)
local NormalScriptURL = "https://raw.githubusercontent.com/roxaspp/JoshHub/refs/heads/main/JoshHubNormalKeyLoadstring"
