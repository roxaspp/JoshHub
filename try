StopBtn.Size = UDim2.new(1, -10, 0, 25)
StopBtn.Position = UDim2.new(0, 5, 1, -28)
StopBtn.BackgroundColor3 = Color3.fromRGB(200, 50, 50)
StopBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
StopBtn.Font = Enum.Font.SourceSansBold
